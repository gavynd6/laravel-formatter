{
    "name": "atroposmental/laravel-formatter",
    "type": "library",
    "description": "A formatting library that converts data output between XML, CSV, JSON, TXT, YAML and a few others.",
    "keywords": ["laravel", "formatter", "data", "convert", "csv", "xml", "yaml"],
    "homepage": "http://github.com/atroposmental/laravel-formatter",
    "license": "MIT",
    "version": "3.5.5",
    "authors": [
        {
            "name": "Dustin Hansen",
            "email": "dustin@atroposmental.com",
            "homepage": "https://atroposmental.com"
        }
    ],
    "require": {
        "php": "^8.0 || ^8.1 || ^8.2",
        "league/csv": "~9.0",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^9.5.11 || ^10.0 || ^11.0"
    },
    "autoload": {
        "psr-4": {
            "Atroposmental\\Formatter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Formatter": "Atroposmental\\Formatter"
            }
        }
    },
    "minimum-stability": "stable"
}
